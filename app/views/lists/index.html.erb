<h1>Lists</h1>

<% @lists.each do |list|%>
  <li>
    <%= list.name %>
    <% if list.photo.attached? %>
      <%= cl_image_tag list.photo.key, height: 150%>
    <% end %>
    <p><%= link_to 'See', list %></p>
  </li>
<% end %>

<%= link_to "New List", new_list_path, class:"btn btn-success my-4" %>
